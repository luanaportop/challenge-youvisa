{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "token": {
      "type": "string"
    },
    "channel": {
      "type": "string",
      "enum": [
        "telegram",
        "whatsapp",
        "web"
      ]
    },
    "user_id": {
      "type": "string"
    },
    "expires_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "token",
    "channel",
    "user_id",
    "expires_at"
  ]
}