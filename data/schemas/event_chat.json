  "type": "object",
  "properties": {
    "event_id": {
      "type": "string"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "channel": {
      "type": "string"
    },
    "user_id": {
      "type": "string"
    },
    "intent": {
      "type": "string"
    },
    "entities": {
      "type": "object"
    },
    "handoff": {
      "type": "boolean"
    }
  },
  "required": [
